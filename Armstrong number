// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,r,n1,sum=0;
    printf("Enter the number:");
    scanf("%d",&n);
    n1=n;
    while(n>0){
        r=n%10;
        sum=sum+r*r*r;
        n=n/10;
    }
    if(n1==sum)
    {
        printf("Armstrong number");
    }
    else{
        printf("Not a armstrong number");
    }
    
    return 0;
}
